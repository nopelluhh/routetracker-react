        "immutable": "^3.8.1",
        "import-glob-loader": "^1.1.0",
        "jest": "^20.0.1",
        "method-override": "^2.3.7",
        "mongoose": "^4.9.1",
        "morgan": "^1.8.1",
        "node-sass": "^4.5.0",
        "passport": "^0.3.2",
        "passport-local": "^1.0.0",
        "postcss-loader": "^1.3.3",
