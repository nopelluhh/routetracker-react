{
    "name": "routetracker",
    "version": "0.1.0",
    "description": "for tracking routes",
    "main": "server.js",
    "scripts": {
        "heroku-postbuild": "webpack -p --progress --colors",
        "test": "jest --watchAll",
        "start": "node server",
        "start:dev": "concurrently -k 'NODE_ENV=development webpack-dev-server' 'node server' 'jest --watchAll'",
        "start:hot": "concurrently -k 'NODE_ENV=development webpack-dev-server' 'node server'",
        "start:wp": "NODE_ENV=development webpack-dev-server",
        "start:mobile": "HOST=172.20.10.2 yarn start"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nopelluhh/routetracker.git"
    },
    "author": "noah peller",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nopelluhh/routetracker/issues"
    },
    "jest": {
        "verbose": true,
        "moduleDirectories": [
            "node_modules",
            "public"
        ]
    },
    "homepage": "https://github.com/nopelluhh/routetracker#readme",
    "dependencies": {
        "autoprefixer": "^6.7.7",
        "babel": "^6.23.0",
        "babel-core": "^6.24.0",
